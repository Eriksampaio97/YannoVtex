{
  "footer": {
    "blocks": [
      "footer-layout.desktop#yanno"
    ]
  },
  "footer-layout.desktop#yanno": {
    "children": [
      "flex-layout.row#footer-logo",
      "flex-layout.row#footer-menu",
      "flex-layout.row#footer-link",
      "flex-layout.row#footer-copyright"
    ],
    "props":{"blockClass": "footer-yanno"}
  },
  "image#logo-yanno":{
    "title": "Logo Footer",
    "props": {
      "src": "https://socialsahomolog.vtexassets.com/assets/vtex.file-manager-graphql/images/c555cd3b-f2f0-4eb1-a970-19c5b8e2da58___bcc0ff36c62e9561d917f99cc4a1aaa5.png",
      "blockClass": "logo-footer"
    }
  },
  "flex-layout.row#footer-logo": {
    "children": [
      "image#logo-yanno"
    ],
    "props": {
      "blockClass": "row-yanno-footer-logo",
      "paddingTop": 6,
      "paddingBottom": 3
    }
  },
  "flex-layout.row#footer-menu": {
    "children": [
      "vtex.menu@2.x:menu#my-account",
      "vtex.menu@2.x:menu#products",
      "vtex.menu@2.x:menu#about",
      "vtex.menu@2.x:menu#need-help"
    ],
    "props": {
      "blockClass": "row-yanno-footer",
      "paddingTop": 6,
      "paddingBottom": 3
    }
  },
  "flex-layout.row#footer-link": {
    "children": [
      "flex-layout.col#social-media",
      "flex-layout.col#payments-method",
      "flex-layout.col#organization"
    ],
    "props": {
      "blockClass": "payment-methods"
    }
  },
  "flex-layout.col#social-media":{
    "title": "Redes Sociais",
    "props": {"blockClass": "col-social-media"},
